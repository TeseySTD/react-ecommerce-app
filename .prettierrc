{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 80,
    "useTabs": false,
    "endOfLine":"auto",
    "overrides": [
        {
            "files": ["*.ts"],
            "options": {
                "tabWidth": 4
            }
        },
        {
            "files": ["*.tsx"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}